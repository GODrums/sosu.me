        <com.example.sudokusolver.ProximaCheckBox
            android:id="@+id/buttonTarget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:clickable="true"
            android:focusable="true"
            android:text="@string/targeting"
            android:onClick="checked"
            android:theme="@style/checkBoxStyle"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/numbers"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

        <com.example.sudokusolver.ProximaButton
            android:id="@+id/buttonComp"
